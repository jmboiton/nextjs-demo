    "eslint-config-next": "15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "json-server": "0.17.4",
    "prettier": "^3.3.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^2.1.4"
  }
